out_to_x no
own_window no
out_to_console yes
background no
max_text_width 0
# Update interval in seconds
update_interval 0.3
total_run_times 0
override_utf8_locale yes

TEXT
[
  #${if_match "${exec ~/.config/music.sh}" == "-"}
  #${else}
    #{ "full_text" : "music:" , "color" : "\#19C103" , "separator" : false , "separator_block_width" : 6 } ,
    #{ "full_text" : "${exec ~/.config/music.sh}" } ,
  #${endif}
  #{ "full_text" : "brightness:" , "color" : "\#19C103" , "separator" : false , "separator_block_width" : 6 } ,
  #{ "full_text" : "${exec xbacklight | cut -d '.' -f1}%" } ,

  { "full_text" : "vol:" , "color" : "\#19C103" , "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "${mixerbar}" } ,
  { "full_text" : "wifi:" , "color" : "\#19C103" , "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "${wireless_essid wlp3s0} ${addr wlp3s0}" } ,
  { "full_text" : "bat:" , "color" : "\#19C103" , "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "${battery BAT0}" , "color" : ${if_match ${battery_percent BAT0}<20}"\#dc322f"${else}"\#fdf6e3"${endif} } ,
  { "full_text" : "date:" , "color" : "\#19C103" , "separator" : false , "separator_block_width" : 6 } , 
  { "full_text" : "${time %d-%b-%Y %I:%M%P}" }
],
