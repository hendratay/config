out_to_x no
own_window no
out_to_console yes
background no
max_text_width 0
# Update interval in seconds
update_interval 0.3
total_run_times 0
override_utf8_locale yes

TEXT
[
  { "full_text" : "" , "color" : "\#fdf6e3" , "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "${exec xbacklight | cut -d '.' -f1}%" } ,
  { "full_text" : "\u266c" , "color" : "\#fdf6e3" , "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "${exec ~/.config/music.sh}" } ,

  { "full_text" : "\uf028" , "color" : "\#fdf6e3" , "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "${mixerbar}" } ,
  { "full_text" : "\uf1eb" , "color" : "\#fdf6e3" , "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "${wireless_essid wlp3s0} (${addr wlp3s0})" } ,
  { "full_text" : "\uf240" , "color" : "\#fdf6e3" , "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "${battery BAT0}" , "color" :
                    ${if_match ${battery_percent BAT0}<20}"\#dc322f"
                    ${else}"\#93a1a1"
                    ${endif} } ,
  { "full_text" : "\uf073" , "color" : "\#fdf6e3" , "separator" : false , "separator_block_width" : 6 } , 
  { "full_text" : "${time %d-%b-%Y}", "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "\uf017" , "color" : "\#fdf6e3" , "separator" : false , "separator_block_width" : 6 } , 
  { "full_text" : "${time %H:%M:%S}" } ,
  { "full_text" : "" , "color" : "\#fdf6e3" , "separator" : false , "separator_block_width" : 6 } ,
  { "full_text" : "${user_names}" }
],
